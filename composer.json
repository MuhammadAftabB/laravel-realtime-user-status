{
    "name": "muhammadaftabb/laravel-realtime-user-status",
    "description": "A Laravel package for real-time user online status",
    "type": "library",
    "authors": [{
        "name": "Muhammad Aftab",
        "email": "your-email@example.com"
    }],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "pusher/pusher-php-server": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MuhammadAftab\\RealTimeUserStatus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadAftab\\RealTimeUserStatus\\RealTimeUserStatusServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}